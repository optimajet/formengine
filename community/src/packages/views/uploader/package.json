{
  "name": "@react-form-builder/components-uploader",
  "description": "OptimaJet Form Builder Components - Uploader",
  "version": "7.12.0",
  "license": "MIT",
  "private": false,
  "author": "Optimajet",
  "homepage": "https://formengine.io/",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/optimajet/formengine.git"
  },
  "main": "src/index.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./schemas/uploader-components.schema.json": "./public/schemas/uploader-components.schema.json"
  },
