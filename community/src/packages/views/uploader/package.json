    "react": ">=17.0.2 || ^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@react-form-builder/core": {
      "optional": false
    }
  },
  "devDependencies": {
    "@react-form-builder/json-schema-generator": "^7.12.0",
    "vite-node": "^5.2.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
